<script type="text/javascript">
 
	var num = 1; 	
	var str = '1'; 	
 	
 	
	// == : 값 비교 
	document.write(num == str);		//true
 	
	// === : 값 & 타입 비교 
 	document.write(num === str);	//false
 	 
 	//부정연산자
 	document.write(num != str);	 
 	document.write(num !== str);		 
 	
</script>
   
</html>